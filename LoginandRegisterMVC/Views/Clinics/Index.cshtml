@model IEnumerable<LoginandRegisterMVC.Models.Clinic>

@{
    ViewBag.Title = "Index";
}

<h2>Add New Clinic</h2>

<p>
    @Html.ActionLink("Create New", "Create",new { id = Session["Id"]})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClinicName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Facilities)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClinicTiming)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressFLine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressSLine)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZipCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClinicName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Facilities)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClinicTiming)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressFLine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressSLine)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id,did= Session["Id"] }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id, did = Session["Id"] }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id, did = Session["Id"] })
        </td>
    </tr>
}

</table>
